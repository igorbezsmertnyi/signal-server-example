{
  "name": "p2p-videochat",
  "version": "1.0.0",
  "description": "P2P videochat using WebRTC and NodeJs signal server",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "copy-assets": "ts-node src/libs/copyAssets",
    "build": "npm-run-all lint tsc copy-assets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorbezsmertnyi/p2p-videochat.git"
  },
  "keywords": [
    "p2p",
    "peer",
    "to",
    "peer",
    "web",
    "rtc",
    "nodejs"
  ],
  "author": "Igor Bezsmertnyi <bezsmertnyi.igor@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/igorbezsmertnyi/p2p-videochat/issues"
  },
  "homepage": "https://github.com/igorbezsmertnyi/p2p-videochat#readme",
  "dependencies": {
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "ws": "^6.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.4",
    "@types/shelljs": "^0.8.2",
    "@types/ws": "^6.0.1",
    "fs-extra": "^7.0.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  }
}
